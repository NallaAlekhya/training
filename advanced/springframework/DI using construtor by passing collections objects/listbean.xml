<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
        <bean id="obj" class="springExample.DenpendencyInjectionOfListValues">
        	<constructor-arg value="1" type="int"/>
        	<constructor-arg value="what is java" type="String"/>
        	<constructor-arg>
        		<list>
        			<value>java is object oriented programming lang</value>
        			<value>java is platform independent</value>
        		</list>
        	</constructor-arg>
        </bean>
        
        <bean id="obj1" class="springExample.DenpendencyInjectionUsingMap">
        
        	<constructor-arg>
        		<map>
        		<entry key="what is java?" value="ganggbhavni"/>
        		<entry key="what is exception handling?" value="satish"/>
        		</map>
        	</constructor-arg>
        </bean>
 </beans>