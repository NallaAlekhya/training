<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
        
        
        <bean id="hello" class="eventListener.Hello">
   		<property name="msg" value="welcome"/>
   		</bean>
   
    <bean id="start" class="eventListener.StartEvent"></bean>
    <bean id="stop" class="eventListener.StopEvent"></bean>
   	<bean id="refresh" class="eventListener.RefreshEvent"></bean>
   	<bean id="close" class="eventListener.CloseEvent"></bean>
   	<bean id="request" class="eventListener.RequestEvent"></bean>
    
</beans>